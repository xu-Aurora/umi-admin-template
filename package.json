    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "qs": "^6.10.3",
    "react": "18.x",
    "react-dom": "18.x",
    "table-render": "^1.3.3",
    "umi": "^3.5.24"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.24",
    "antd-dayjs-webpack-plugin": "^1.0.6",
